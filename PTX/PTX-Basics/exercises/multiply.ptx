//
// Simple multiplication kernel in PTX
// Multiplies two integers and stores the result
//

.version 6.0
.target sm_50
.address_size 64

.visible .entry multiply_simple(
    .param .u32 a,
    .param .u32 b,
    .param .u32* result
) {
    .reg .u32 %a1, %b1, %product;
    .reg .u64 %result_ptr;
    
    // Load parameters
    ld.param.u32 %a1, [a];
    ld.param.u32 %b1, [b];
    
    // Perform multiplication
    mul.lo.u32 %product, %a1, %b1;
    
    // Load pointer to result
    ld.param.u64 %result_ptr, [result];
    
    // Store result
    st.u32 [%result_ptr], %product;
    
    ret;
}